CREATE TABLE modalidade(       #criar_Tabela
ID_modalidade INT NOT NULL AUTO_INCREMENT, #
nome VARCHAR(200) NOT NULL, #nãoVazio
valor DOUBLE  NOT NULL,
PRIMARY KEY(ID_modalidade)) ;


